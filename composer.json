{
    "name": "laravel-authorize",
    "type": "library",
    "description": "Authorize.Net payment gateway driver for the Omnipay 3.x payment processing library",
    "keywords": [
        "authorize.net",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://gitlab.com/cimpleo-sandbox/laravel-authorize",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Sidorov",
            "email": "denis.sidorov@cimpleo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Authomnipay\\": "src/"
        }
    },
    "require": {
        "omnipay/common": "^3",
        "academe/authorizenet-objects": "^0.7",
        "symfony/property-access": "^3.2"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "branch-alias": {
            "master": "1.0.x-dev"
        }
    }
}
